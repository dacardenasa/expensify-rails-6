$('table tbody tr#' + <%= j @id  %>).remove();
$('.alerta').html("Expense deleted").show("slow");

setTimeout(function(){
  $('.alerta').hide("slow");
}, 2000);

$("#total").html("<%= j Expense.all.sum(:amount) %>")
$("#transactions").html("<%= j Expense.all.count() %>")
$("#average").html("<%= j Expense.all.sum(:amount) / Expense.all.count() %>")



